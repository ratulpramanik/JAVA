package Patterns;

public class RPrattern {
	public static void main(String args[]) {
		int i,j,k;
		for(i=1;i<=3;i++) {
			for(j=1;j<=4;j++) {
				if((i==2) && (j==2||j==3)) {
					System.out.print(" ");
				}
				else
				System.out.print("*");
			}
			
			System.out.println();
		}
		
		for(i=1;i<=3;i++) {
			for(j=1;j<=4;j++) {
				if((i==2) && (j==2||j==3)) {
					System.out.print(" ");
				}
				else
				System.out.print("*");
			}
			
			System.out.println();
		}
		}
	}


