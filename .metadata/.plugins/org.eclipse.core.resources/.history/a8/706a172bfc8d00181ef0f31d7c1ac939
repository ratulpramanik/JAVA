package Patterns;

public class Try {
	public static void main(String args[]) {
		int i,j,k;
		for(i=1;i<=5;i++) {
			for(j=1;j<=3;j++) {
				if(j==2) {
					System.out.print(" "+"*");
				}
				if(j==4) {
					for(k=1;k<=2;k++) {
						System.out.print(" ");
					}
					
					{
				System.out.print("*");
				}
			}
			System.out.println();
		}
	}
}
	}
