package Patterns;

public class face {
		public static void main(String args[]) {
			int i,j;
			for(i=1;i<=5;i++) {
				for(j=1;j<=9;j++) {
					if(i==2||i==5) {
						System.out.print("-");
					}
					else if(i==2 &&(j==3 ||j==6)) {
						System.out.print("0");
					}
					else if(i==3 &&(j==3||j==4||j==5||j==6)) {
						System.out.print("*");
					}
					else
						System.out.print(" ");
				
				if(j==1||j==9) {
					System.out.print("|");
				}
				else
				{
					System.out.print(" ");
				}
				}
				System.out.println();
			}
		}
}
